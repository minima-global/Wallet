{
    "name": "wallet",
    "version": "2.46.1",
    "private": true,
    "type": "module",
    "description": "Exchange tokens with Minimalists",
    "scripts": {
        "dev": "vite",
        "build": "tsc && GENERATE_SOURCEMAP=false vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
