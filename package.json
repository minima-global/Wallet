        "@types/react-dom": "^17.0.11",
        "@types/sanitize-html": "^2.8.1",
        "@vitejs/plugin-legacy": "^4.1.1",
        "@vitejs/plugin-react": "^4.1.0",
        "@vitejs/plugin-react-swc": "^3.4.0",
        "tailwindcss": "^3.3.3",
        "vite": "^4.4.11"
