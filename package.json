        "@mui/icons-material": "^5.3.1",
        "@mui/material": "^5.3.1",
        "@mui/system": "^5.3.0",
        "@react-spring/web": "^9.7.3",
        "@reduxjs/toolkit": "^1.8.3",
        "autoprefixer": "^10.4.16",
        "date-fns": "^2.29.3",
        "decimal.js": "^10.4.2",
        "env-cmd": "^10.1.0",
        "flowbite": "^1.8.1",
        "format-number": "^3.0.0",
        "formik": "^2.2.9",
        "http-proxy-middleware": "^2.0.2",
        "lottie-react": "^2.4.0",
